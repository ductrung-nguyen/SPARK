package learnscala1

import test._

object test7 {
  val list = new Cons(1, new Cons(2, new Cons(3, new Nil)))
                                                  //> list  : learnscala1.Cons[Int] = learnscala1.Cons@73d9024f
  
  list.findElementAtIndex(0)                      //> res0: Int = 1
  
}

object List {
	def apply[T](x1 : T, x2 : T) = new Cons(x1, new Cons(x2, new Nil))
	def apply[T]() = new Nil
}