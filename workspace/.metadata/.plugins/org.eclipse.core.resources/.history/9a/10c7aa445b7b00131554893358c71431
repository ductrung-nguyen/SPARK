package spark

import collection.immutable.TreeMap

object TestingWorkSheet {

 	var l = List(
 		(1,List(1,2)),
 		(3,List(4,5)),
 		(5,List(6,7)))                    //> l  : List[(Int, List[Int])] = List((1,List(1, 2)), (3,List(4, 5)), (5,List(6
                                                  //| , 7)))
 	l.map(x => (x._1, x._2.sum()))            //> res0: List[(Int, List[Int])] = List((1,List(1, 2)), (3,List(4, 5)), (5,List(
                                                  //| 6, 7)))
}